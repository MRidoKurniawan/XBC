@{
    ViewBag.Title = "Feedback";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3>@ViewBag.Title</h3>
    </div>

    <div id="data-list">
        <h4>Loading....</h4>
    </div>
</div>



<div id="myModal" class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background-color:#428bca; color:white">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Modal title</h4>
            </div>
            <div class="modal-body">
                <p>One fine body&hellip;</p>
            </div>
            <div class="modal-footer">
                @*<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save changes</button>*@
            </div>
        </div><!-- /.modal-content -->
        <div class="modal-content modal2" style="display:none; position:relative; top: -25rem; z-index:999; padding:3rem;">
            <div class="modal-header header2" style="background-color:#428bca; color:white">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title title2">Modal title</h4>
            </div>
            <div class="modal-body body2">
                <p>One fine Body&hellip;</p>
            </div>
        </div>
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
@section Scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            GetList();
        });

        function GetList() {
            $.ajax({
                url: '@Url.Action("List","Feedback")',
                type: 'get',
                success: function (result) {
                    $("#data-list").html(result);
                }

            });

        }


    </script>
}