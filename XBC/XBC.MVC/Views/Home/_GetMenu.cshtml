@model IEnumerable<XBC.ViewModel.MenuViewModel>

@foreach (var item in Model)
{
    
    <li>
        <a href="http://localhost:1216/@item.menu_url">
            <div style="">
                @if (item.image_url != null)
                {
                    <img src="@item.image_url" style="height:20px; width:auto;"/>
                }
                @item.title
            </div>
        </a>
    </li>
}
<li><a href="http://localhost:1216/Home/Logout">Log Out</a></li>
