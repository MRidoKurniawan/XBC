@{
    ViewBag.Title = "Biodata";
    //ViewBag.Subtitle = "List of";
}
<div class="panel panel-primary">
    <div class="panel-heading">
        <h3>Biodata</h3>
    </div>
    <div id="data-list" class="panel-body">
        <h4>Loading...</h4>
    </div>
</div>

<div class="modal fade modal-umum" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title modal-umum-title">Modal title</h4>
            </div>
            <div class="modal-body modal-umum-body">
                <p>One fine body&hellip;</p>
            </div>
            @*<div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save changes</button>
                </div>*@
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<div class="modal fade modal-edit" tabindex="-1" role="dialog">
    <div style="width:80rem" class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title modal-edit-title">Modal title</h4>
            </div>
            <div class="modal-body modal-edit-body">
                <p>One fine body&hellip;</p>
            </div>
            @*<div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save changes</button>
                </div>*@
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

@section Scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            GetList();
        });

        function GetList() {
            $.ajax({
                url: '@Url.Action("List", "Biodata")',
                type: 'get',
                success: function (result) {
                    $("#data-list").html(result);
                }
            });
        }
    </script>
}



